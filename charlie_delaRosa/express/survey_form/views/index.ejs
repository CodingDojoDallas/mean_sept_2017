<h1>Survey Form</h1>
<form class="" action="/result" method="post">
  <table>
    <tr>
      <td><label for="">Name:</label></td>
      <td><input type="text" name="name" value=""></td>
    </tr>
    <tr>
      <td><label for="">Dojo Location:</label></td>
      <td><select class="" name="location">
        <option value="Seattle">Seattle</option>
        <option value="Dallas">Dallas</option>
        <option value="San Jose">San Jose</option>
      </select></td>
    </tr>
    <tr>
      <td><label for="">Favorate Language:</label></td>
      <td><select class="" name="fav_lang">
        <option value="JavaScript">JavaScript</option>
        <option value="Python">Python</option>
        <option value="Ruby">Ruby</option>
        <option value="Java">Java</option>
      </select></td>
    </tr>
    <tr>
      <td><label for="">Comment (optional):</label></td>
    </tr>
    <tr>
      <td><textarea name="comment" rows="4" cols="25"></textarea></td>
    </tr>
    <tr>
      <td><input type="submit" name="submit" value="Submit"></td>
    </tr>
  </table>
</form>
